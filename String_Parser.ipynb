    "def string_parse(string):\n",
    "    ch_dict={}\n",
    "    assert(isinstance(string, str)),'Input needs to be a string.'\n",
    "    for ch in string:\n",
    "        try:\n",
    "            ch_dict[ch]+=1\n",
    "        except:\n",
    "            ch_dict[ch]=1\n",
    "    return ch_dict"
   ]
